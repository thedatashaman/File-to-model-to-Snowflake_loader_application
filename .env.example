# Snowflake Configuration
# Copy this file to .env and fill in your values

# Required for password authentication
SNOWFLAKE_ACCOUNT=your_account.us-east-1
SNOWFLAKE_USER=your_username
SNOWFLAKE_PASSWORD=your_password

# Required for key-pair authentication (alternative to password)
# SNOWFLAKE_PRIVATE_KEY=/path/to/private_key.pem
# Or paste the PEM content directly:
# SNOWFLAKE_PRIVATE_KEY=-----BEGIN PRIVATE KEY-----\n...\n-----END PRIVATE KEY-----

# Optional but recommended
SNOWFLAKE_WAREHOUSE=COMPUTE_WH
SNOWFLAKE_DATABASE=MY_DATABASE
SNOWFLAKE_SCHEMA=MY_SCHEMA
SNOWFLAKE_ROLE=ACCOUNTADMIN
